}
.commodity_list{
  width: 100%;
  margin-top: 20rpx;
}
.swiper-tab{  
    width: 100%;
    background: #fff;
    border-bottom: 2rpx solid #ecf4fa;  
    text-align: center;  
    line-height: 100rpx;
}  
    
