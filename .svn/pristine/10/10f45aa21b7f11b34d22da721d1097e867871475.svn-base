    }  

.boutique_list{
  width: 94%;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  /*height: 950rpx;*/
}
.boutique_details{
  margin-top: 32rpx;
  height: 220rpx;
  border-radius: 10rpx;
  background: #fff;
  box-shadow: #DAE3E9 0 0 20rpx ;
  padding: 20rpx;
  display: flex;
  flex-direction: row;
 
}
.boutique_img{
  text-align: center;
  width: 200rpx;
  border: 1rpx #EFEFEF solid;
  height: 200rpx;
  display: table-cell;
  vertical-align:middle;
  position: relative;
}
.activity_img {
  background: url(http://img.aipaike.com/group1/M00/10/23/Cvv7ZVk2dvOAAu2JAAAy8vIagyY627.png) bottom no-repeat;
  background-size: contain
}
.storeshinee{
  background: url(http://img.aipaike.com/group1/M00/10/22/Cvv7ZVk2Wo6ABW7AAAAMqdFRiA0292.png) no-repeat;
  width: 100rpx;
  height: 100rpx;
  background-size: contain;
  position: absolute;
  display: block;
  top: 0;
  right: 0;
}
.shoe_img{
  width: 80%;
  height: 80%;
  margin: 0 auto;
  vertical-align:middle;
}
.navigator_details{
  display: flex;
  flex-direction: row;
  width: 100%;
}
.boutique_info{
  display: flex;
  flex-direction: column;
  padding: 20rpx;
  width: 470rpx;
  
}
.boutique_title{
  height: 135rpx;
  text-align: left;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #000000;
}
.price_pay{
  position: relative;
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
  height: 80rpx;
}
.price{
  font-size: 34rpx;

  float: left;
  line-height: 60rpx;
}
.member_icon{
  height: 40rpx;
  width: 55rpx;
  margin: 15rpx 0 0 10rpx;
  color: #fff;
  font-size: 22rpx;
  text-align: center;

  line-height: 40rpx;
  border-radius: 10rpx;
}
.member_bg{
  background: #FF6F07;
}
.selling_bg{
  background: #FB2F2E;
}
.rebate_bg{
  background: #6DB127;
  width: 75rpx;
}
.pay{
  width: 100rpx;
  position: absolute;
  right: 0;
  float: right;
  text-align: right;
  margin-top: -10rpx;
}
.pay_img{
  width: 80rpx;
  height: 80rpx;
}
.disnone{
  display: none;
}
.lodingmore{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #777777;
  position: relative;
  bottom: 0rpx;
}
.nearshop {
  background: #fff;
  height: 100%;
}

.searchNearshop {
  width: 738rpx;
  height: 96rpx;
  border-bottom: solid 2rpx #f6f6f6;
  overflow: hidden;
  margin: 0 auto 10rpx;
  position: relative;
}

.nearshop_icon {
  position: absolute;
  background: url(http://img.aipaike.com/group1/M00/10/34/Cvv7ZVlR_MuAQIkVAAAFCHViUSA865.png) no-repeat;
  background-size: contain;
  display: block;
  width: 33rpx;
  height: 33rpx;
  left: 54rpx;
  top: 33rpx;
}

.nearshop_inp {
  width: 85%;
  padding-left: 100rpx;
  font-size: 26rpx;
  height: 100%;
  line-height: 100%;
}

.nearshop_loc2 {
  position: absolute;
  display: block;
  width: 28rpx;
  height: 40rpx;
  right: 24rpx;
  top: 9rpx;
  padding: 20rpx 30rpx;
}

.nearshop_loc {
  background: url(http://img.aipaike.com/group1/M00/10/34/Cvv7ZVlR_MuAMHrDAAAEXCQi1FU939.png) no-repeat;
  background-size: contain;
  display: block;
  width: 28rpx;
  height: 40rpx;
}

.nearshop_list {
  width: 738rpx;
  margin: 0 auto;
  height: 950rpx;
}

.nearshop_details {
  box-shadow: 0 0 24rpx 0 rgba(217,226,232,0.50);
  border-radius: 8rpx;
  width: 690rpx;
  margin: 30rpx 24rpx 6rpx 24rpx;
  position: relative;
  box-sizing: border-box;
  padding: 55rpx 30rpx 36rpx 60rpx;
}

.nearshop_info {
  display: flex;
  flex-direction: row;
  height: 120rpx;
}

.nearshop_info image {
  width: 114rpx;
  height: 114rpx;
  display: block;
  border-radius: 50%;
  overflow: hidden;
}

.nearshop_na {
  width: 456rpx;
  padding-left: 30rpx;
}

.nearshop_name {
  font-size: 28rpx;
  display: block;
  line-height: 54rpx;
}


.nearshop_address {
  font-size: 24rpx;
  display: block;
  line-height: 36rpx;
}

.nearshop_address icon {
  float: left; 
  display: block;
  width: 10rpx;
  height: 10rpx;
  background: url(http://img.aipaike.com/group1/M00/10/34/Cvv7ZVlSBd6AUzjdAAAA3tuiBCc035.png) no-repeat;
  background-size: contain;
  margin: 15rpx 8rpx 0 0;
}

.nearshop_location {
  position: absolute;
  background: url(http://img.aipaike.com/group1/M00/10/34/Cvv7ZVlSBd6ARRmTAAAGqPFIP8g461.png) no-repeat;
  background-size: contain;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  right: 30rpx;
  top: 0;
  width: 100rpx;
  height: 98rpx;
  overflow: hidden;
  padding-top: 46rpx;
  line-height: 30rpx;
}

.nearshop_phone icon {
  float: left;
  width: 84rpx;
  height: 125rpx;
  background: url(http://img.aipaike.com/group1/M00/10/34/Cvv7ZVlSCQmAH1ZLAAC7kpa3Wis416.png) no-repeat;
  background-size: contain;
  margin: -6rpx 45rpx 0 15rpx;
}

.nearshop_phone {
  display: block;
  height: 125rpx;
  width: 100%;
}

.nearshop_phone .nearshop_text {
  display: block;
  font-size: 24rpx;
  color: #c6c6c6;
  line-height: 30rpx;
  padding-top: 36rpx;
}

.nearshop_phone .nearshop_num {
  font-size: 30rpx;
  color: #515151;
  margin-top: 10rpx;
}

.nearshop_but {
  position: absolute;
  right: 35rpx;
  bottom: 35rpx;
  border: 4rpx solid #FCE8E8;
  border-radius: 200rpx;
  width: 187rpx;
  height: 74rpx;
  line-height: 68rpx;
  font-size: 28rpx;
  color: #FB2F2E;
  text-align: center;
  display: block;
  background: #fff;
  box-sizing: border-box;
}

.nonearshop {
  text-align: center;
  font-size: 30rpx;
  line-height: 60rpx;
  color: #888;
}

.none {
  display: none;
}