@import "../common-template/coupon-number-template/coupon-number-template.wxss";

.swiper-tab{  
    width: 100%;  
    border-top: 2rpx solid #F6F6F6;
    text-align: center;  
    line-height: 100rpx;
    background: #fff;
    height: 100rpx;
    }  
.swiper-tab-list{  
    font-size: 28rpx;  
    display: inline-block;  
    width: 33.33%;  
    color: #666666;  
}  
.on{ 
      color: #FB2F2E ;  
      border-bottom: 5rpx solid #FB2F2E; 
      position: relative;
      top: -5rpx;
    }   
.swiper-box{ 
  display: block; 
  height: 100%; 
  width: 100%;
  overflow: hidden;
}  
.swiper-box view{  
     
}  

.coupon_details{
  width: 94%;
  margin: 0 auto;
  background: #fff;
  height: 260rpx;
  margin-top: 40rpx;
  display: flex;
  flex-direction: row;
  position: relative;
}
.coupon_details_left{
  width: 38%;
  background: url(http://img.aipaike.com/group1/M00/10/2B/Cvv7ZVlB8QiANx7lAAAyY-T4QwM564.png) no-repeat;
  height: 260rpx;
  background-size: contain;  
}
.coupon_details_left_overdue{
  width: 38%;
  background: url(http://img.aipaike.com/group1/M00/10/2B/Cvv7ZVlCL7yALQX3AAAMqisb3YU900.png) no-repeat;
  height: 260rpx;
  background-size: contain;  
}
.coupon_details_left_num{
  width: 100%;
  text-align: center;
  color: #fff;
font-size: 60rpx;
text-indent: -0.4em;
}
.coupon_details_left_explain{
  width: 100%;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  text-indent: -0.4em;
}
.coupon_details_left_centent{
  position: relative;
  top: 20%
}
.coupon_details_right{
  width: 62%;
  padding-top: 20rpx;
  display: flex;
  flex-direction: column;
}
.coupon_details_right_jjdq{
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  height: 100rpx;
  width: 100rpx;
  background: url(http://img.aipaike.com/group1/M00/10/23/Cvv7ZVk2Wu2AFnoiAAAKrRcdWpk131.png) no-repeat;
  background-size:cover; 
  z-index: 999;
}
.coupon_details_right_top {
  width: 100%;
  display: flex;
  flex-direction: row;
  text-align: left;
  
}
.coupon_txt{
  width: 90%;
  padding-right: 10rpx;
  text-indent: 5.5em;
  position: relative;
  top: -40rpx;
  color: #4F4F4F;
  height: 60rpx;
  font-size: 28rpx;
}
.coupon_icon_md{
  background: #22CFA8;
  height: 40rpx;
  width: 120rpx;
  color: #fff;
  font-size: 28rpx;
  border-radius: 5rpx;
  text-align: center;
}
.coupon_icon_md_overdue{
  background: #fff;
  height: 40rpx;
  width: 120rpx;
  color: #B9B9B9;
  font-size: 28rpx;
  border-radius: 5rpx;
  text-align: center;
  border: 1px solid #B9B9B9
}
.coupon_icon_scmd{
  background: #F6A623;
  height: 40rpx;
  width: 135rpx;
  color: #fff;
  font-size: 28rpx;
  border-radius: 5rpx;
  text-align: center;
}
.coupon_user_date{
  width: 100%;
  display: flex;
  flex-direction: row;
  position: relative;
  top: -60rpx;
  
}
.user_date{
  width: 60%;
  color: #656565;
  font-size: 25rpx;
  font-weight: lighter;
  height: 60rpx;
  line-height: 120rpx;
}
.ticket_number{
  height: 50rpx;
  width: 130rpx;
  border: 1px solid #FE4A10;
  color: #FE4A10;
  text-align: center;
  line-height: 50rpx;
  border-radius: 30rpx;
  position: relative;
  top: 30rpx;
  margin-left: 10rpx;
  font-size: 28rpx;
}
.use_explain{
  width: 100%;
  display: flex;
  flex-direction: row;
  height: 60rpx;
  border-top: 1rpx solid #F5F5F5;
 
  
}
.use_explain_txt{
  line-height: 80rpx;
  color: #838383;
  width: 85%;
  font-size: 26rpx;
}
.use_explain_arrow{
  width:30rpx;
  height: 30rpx;
  background: url(http://img.aipaike.com/group1/M00/10/23/Cvv7ZVk2Wu6AbBaOAAAA5p1kfqQ437.png) no-repeat;
   background-size: contain;
   position: relative;
  top: -50rpx;
  left: 100%;
  /*margin-top: 30rpx;*/
}
.use_explain_arrow_up{
  width:30rpx;
  height: 30rpx;
  background: url(http://img.aipaike.com/group1/M00/10/23/Cvv7ZVk2Wu6AX7RxAAAA5dED680156.png) no-repeat;
  background-size: contain;
  position: relative;
  top: -50rpx;
  left: 100%;
  /*margin-top: 30rpx;*/
}
.use_explain_zk{
  width: 95%;
  margin: 0 auto;
  margin-top: 10rpx;
  background: #fff;
  padding-bottom: 20rpx;
  padding-top: 20rpx
}
.use_explain_zk_content{
  width: 92%;
  margin: 0 auto;
  font-size: 28rpx;
}
.activation{
  margin-top: 30rpx;
  height: 120rpx;
  width:100%;
  display: flex;
  flex-direction: row;
  background: #fff;
  position: fixed;
  bottom: 0;
}
.coupon_num{
  width: 70%;
  height: 120rpx;
}
.coupon_num_input{
  line-height: 120rpx;
  height: 120rpx;
  width: 90%;
  margin: 0 auto;
  font-size: 28rpx;
}
.activation_button{
  width: 30%;
  height: 120rpx;
  background: #FB2F2E;
  line-height: 120rpx;
  color: #fff;
  text-align: center;
}
.have_use{
  height: 120rpx;
  width: 120rpx;
  background: url(http://img.aipaike.com/group1/M00/10/23/Cvv7ZVk2Wu6AAfdiAAAq7Xc6u-w161.png) no-repeat;
  background-size: cover;
  position: absolute;
  top: 80rpx;
  right: 10rpx;
}
.have_overdue{
  height: 120rpx;
  width: 120rpx;
  background: url(http://img.aipaike.com/group1/M00/10/23/Cvv7ZVk2Wu6AdqsrAAAo3B19dGk013.png) no-repeat;
  background-size: cover;
  position: absolute;
  top: 80rpx;
  right: 10rpx;
}
.have_coupon{
  width: 100%;
  height: 400rpx;
  display: flex;
  flex-direction: column;
}
.have_coupon_bg{
  width: 40%;
  margin: 0 auto;
  position: relative;
  top: 100rpx;
  height: 200rpx;
  background: url(http://img.aipaike.com/group1/M00/10/23/Cvv7ZVk2Wu6APg5DAAAXEDgHMb0492.png) no-repeat;
background-size:contain; 
}
.have_coupon_txt{
  width: 45%;
  height: 40rpx;
  margin: 0 auto;
  text-align: center;
  position: relative;
  top: 120rpx;
  color:#959595;
  font-size: 28rpx;
}
.disnone{
  display: none;
}