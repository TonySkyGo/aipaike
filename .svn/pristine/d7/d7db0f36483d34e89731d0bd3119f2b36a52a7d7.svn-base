/**index.wxss**/
.container{
  width:100%;
  flex-direction:column;  
}
.toplogo{
   background-image: url(http://img.aipaike.com/group1/M00/10/22/Cvv7ZVk2Wo2AVesbAAAnAHQavVU352.png);
  background-size: cover;
  width:100%;
  flex-direction:column;
  height: 330rpx;
}
.logoimgtxt{
  width: 100%;
  position: absolute;
  top: 60rpx; 
}
.img_logo{
  width: 45%;
  background: url(http://img.aipaike.com/group1/M00/10/23/Cvv7ZVk2b0WALS6_AAAdl-xBx2w458.png) no-repeat;
  background-size:cover; 
   margin: 0 auto;
  height: 55rpx;
}
.logImg{
  width: 30%;
  background: url()
}
.searchBar{
  width: 650rpx;
  height: 90rpx;
  position: relative;
  line-height: 100rpx;
  top: 180rpx;
  background: white;
  margin: 0 auto;
  border-radius: 50rpx;
  align-items: center;
}
.search_img{
  width: 50rpx;
  height: 50rpx;
  margin-left: 100rpx;
  margin-top: 25rpx;
  float: left;
}
.search_icon{
  width: 70%;
  height: 100rpx;
  padding-left: 20rpx;
}
.navigation_menu{
  flex-direction: row;
  width: 100%;
  display: flex;
  background: #fff;
}
.coupon{
  width: 33.333%;
  padding-top: 20rpx;
  display: flex;

}
.coupon_img{
   width: 50%;
   background-image: url(http://img.aipaike.com/group1/M00/10/22/Cvv7ZVk2Wo6ABmZiAAATSgxgN7Q196.png);
height: 120rpx;
background-size:cover; 
margin: 0 auto;
}
.rule_line{
  float: right; 
  width: 3rpx; 
  height: 100rpx;
  border-right: 1px solid #ededed;
  margin-top: 5rpx;
  }
.shopping{
  width: 33.3%;
  padding-top: 20rpx;
  display: flex;
}
.shopping_img{
   width: 50%;
   background-image: url(http://img.aipaike.com/group1/M00/10/22/Cvv7ZVk2Wo6AWWSwAAATbb0Pg5Y386.png);
height: 120rpx;
background-size:cover; 
margin: 0 auto;
}
.classify{
  width: 33.3%;
  padding-top: 20rpx;
  display: flex;
}
.classify_img{
   width: 50%;
   background-image: url(http://img.aipaike.com/group1/M00/10/22/Cvv7ZVk2Wo6AFqlWAAAWiiENytQ717.png);
height: 120rpx;
background-size:cover; 
margin: 0 auto;
}
.cllssify_text {
  flex-direction: row;
  width: 100%;
  display: flex;
  background: #fff;
  height: 50rpx;
  padding-top: 10rpx;
  padding-bottom: 15rpx;
  color: #0C0C0C ;
}
.coupon_txt{
  width: 33.3%;
  text-align: center;
}
.shopping_txt{
  width: 33.3%;
  text-align: center;
}
.rule_txt{
  width: 33.3%;
  text-align: center;
}
.commodity_list{
  width: 100%;
  margin-top: 20rpx;
}
.swiper-tab{  
    width: 100%;
    background: #fff;
    border-bottom: 2rpx solid #ecf4fa;  
    text-align: center;  
    line-height: 100rpx;
}  
    
.swiper-tab-list{  
      
    display: inline-block;  
    width: 33.333%;  
    color: #535967;  
}  
.on{
    color: #FE4A10;  
    border-bottom: 5rpx solid #fe4a10;}   
.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden; }  
.swiper-box view{  
    
}  
.boutique_list{
  width: 94%;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  height: 950rpx;
}
.boutique_details{
  margin-top: 20rpx;
  height: 220rpx;
  border-radius: 10rpx;
  background: #fff;
  box-shadow: #777777 0 0 10px;
  padding: 20rpx;
  display: flex;
  flex-direction: row;
  
}
.boutique_img{
  text-align: center;
  width: 200rpx;
  border: 1rpx #EFEFEF solid;
  height: 200rpx;
  display: table-cell;
  vertical-align:middle;
  position: relative;
}
.activity_img {
  background: url(http://img.aipaike.com/group1/M00/10/23/Cvv7ZVk2dvOAAu2JAAAy8vIagyY627.png) bottom no-repeat;
  background-size: contain
}
.storeshinee{
  background: url(http://img.aipaike.com/group1/M00/10/22/Cvv7ZVk2Wo6ABW7AAAAMqdFRiA0292.png) no-repeat;
  width: 100rpx;
  height: 100rpx;
  background-size: contain;
  position: absolute;
  display: block;
  top: 0;
  right: 0;
}
.shoe_img{
  width: 80%;
  height: 80%;
  margin: 0 auto;
  vertical-align:middle;
}
.navigator_details{
  display: flex;
  flex-direction: row;
  width: 100%;
}
.boutique_info{
  display: flex;
  flex-direction: column;
  padding: 20rpx;
  width: 470rpx;
  
}
.boutique_title{
  height: 135rpx;
  text-align: left;
  font-size: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.price_pay{
  position: relative;
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
  height: 80rpx;
}
.price{
  font-size: 40rpx;
  font-weight: bold;
  float: left;
  line-height: 60rpx;
}
.member_icon{
  height: 40rpx;
  width: 55rpx;
  margin: 15rpx 0 0 10rpx;
  color: #fff;
  font-size: 20rpx;
  text-align: center;
  font-weight: lighter;
  line-height: 40rpx;
  border-radius: 10rpx;
}
.member_bg{
  background: #FF6F07;
}
.selling_bg{
  background: #FB2F2E;
}
.rebate_bg{
  background: #6DB127;
  width: 75rpx;
}
.pay{
  width: 100rpx;
  position: absolute;
  right: 0;
  float: right;
  text-align: right;
  margin-top: -10rpx;
}
.pay_img{
  width: 80rpx;
  height: 80rpx;
}
.disnone{
  display: none;
}
.lodingmore{
  width: 100%;
  height: 100rpx;
  line-height: 120rpx;
  text-align: center;
  color: #777777;
}