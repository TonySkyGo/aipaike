/*搜索的热搜css*/
.hot-history-container {
  padding: 54rpx 24rpx;
}

.hot-history-container .hot-history-title {
  font-size: 30rpx;
  color: #191919;
  margin-bottom: 22rpx;
}

.hot-bar{
  float: left;
}

.hot-bar .hot-desc {
  float: left;
  border: 1px solid #f0f0f0;
  width: 150rpx;
  height: 54rpx;
  font-size: 26rpx;
  text-align: center;
  line-height: 54rpx;
  margin-right: 16rpx;
  color: #999;
  margin-bottom: 15rpx;
}

.hot-bar .sel-border{
  border: 1px solid red;
}

